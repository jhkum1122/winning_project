function checkform()
{
	if(f.name.value=="")
		{
		alert("이름을 입력하세요");
		document.f.id.focus();
		return false;
		}
	if(f.ssn1.value=="")
		{
		alert("생년월일을 입력하세요");
		document.f.ssn1.focus();
		return false;
		}
	
	document.f.submit();
	}
	</script>
